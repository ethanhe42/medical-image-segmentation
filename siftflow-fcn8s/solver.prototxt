train_net: "trainval.prototxt"
test_net: "test.prototxt"
test_iter: 20
test_interval: 1000
display: 20
average_loss: 20
lr_policy: "fixed"
# lr for unnormalized softmax
base_lr: 1e-11
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 300000
weight_decay: 0.0005
test_initialization: false
# save model
snapshot: 5000
snapshot_prefix:"/home/yihuihe/medical-image-segmentation"
